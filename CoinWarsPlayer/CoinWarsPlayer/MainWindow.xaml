<Window x:Class="CoinWarsPlayer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoinWarsPlayer"
        mc:Ignorable="d"
        Title="Coin Wars!" Height="450" Width="800"
        Background="Black"
        WindowState="Maximized"
        WindowStyle="None"
        KeyUp="Window_KeyUp"
        Activated="Window_Activated">
    <Grid Name="PlayerGrid">
        <MediaElement Name="CoinWarsPlayer" Opacity="1.0" MediaEnded="CoinWarsPlayer_MediaEnded" LoadedBehavior="Manual" Volume="1" />

    </Grid>
</Window>
